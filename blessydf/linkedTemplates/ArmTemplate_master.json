{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "blessydf"
		},
		"ls_AzureSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_AzureSql'"
		},
		"ls_blob_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_blob'"
		},
		"ls_AzureSql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "stephenserverscd.database.windows.net"
		},
		"ls_AzureSql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "stephen_db"
		},
		"ls_AzureSql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "stephen"
		},
		"ls_blob_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stephenadls2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "blessydf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureSql_password": {
						"value": "[parameters('ls_AzureSql_password')]"
					},
					"ls_blob_accountKey": {
						"value": "[parameters('ls_blob_accountKey')]"
					},
					"ls_AzureSql_properties_typeProperties_server": {
						"value": "[parameters('ls_AzureSql_properties_typeProperties_server')]"
					},
					"ls_AzureSql_properties_typeProperties_database": {
						"value": "[parameters('ls_AzureSql_properties_typeProperties_database')]"
					},
					"ls_AzureSql_properties_typeProperties_userName": {
						"value": "[parameters('ls_AzureSql_properties_typeProperties_userName')]"
					},
					"ls_blob_properties_typeProperties_url": {
						"value": "[parameters('ls_blob_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}